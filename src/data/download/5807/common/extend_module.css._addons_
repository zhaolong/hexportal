/** forum::viewthread,group::viewthread,forum::trade,forum::misc **/
/* 帖子楼层相关的样式 by Pony 1203191047 */

.pl { margin-top: 8px; }
.pl .quote { background-color: #FCFCFC; }
.pl table {}
	.pls, .ad .pls, .ad .plc { border-right: none; background-color: transparent; }
